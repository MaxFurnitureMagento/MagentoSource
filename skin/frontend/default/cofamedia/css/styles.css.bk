/* Global Styles ========================================================================= */
/* Form Elements */
input,select,textarea,button { font:12px Arial, Helvetica, sans-serif; vertical-align:middle; color:#000; }
input.input-text,select,textarea { background:#fff; border:1px solid #ddd; }
input.input-text,textarea { padding:2px; }
select { padding:1px; }
select option { padding-right:10px; }
select.multiselect option { border-bottom:1px solid #ddd; padding:2px 5px; }
select.multiselect option:last-child { border-bottom:0; }
textarea { overflow:auto; }
input.radio { margin-right:3px; }
input.checkbox { margin-right:3px; }
input.qty { width:2.5em !important; }
button.button::-moz-focus-inner { padding:0; border:0; } /* FF Fix */
button.button { -webkit-border-fit:lines; } /* <- Safari & Google Chrome Fix */
button.button { overflow:visible; width:auto; border:0; padding:0; margin:0; background:transparent; cursor:pointer; }
button.button span { color:#111111; height:21px; font:normal 12px/21px Arial, Helvetica, sans-serif; text-align:center; white-space:nowrap; }
button.button > span { background:url('../images/ui/button-gray-left.png') no-repeat center left; padding:0 0 0 2px; display:block; }
button.button > span > span { background:url('../images/ui/button-gray-right.png') no-repeat center right; padding:0 8px 0 6px; display:block; }
button.disabled {}
button.disabled span {}

button.btn-checkout span { background:transparent url(../images/ui/proceed-to-checkout.png) no-repeat 0 0; width:209px; height:39px; padding:0; }
button.btn-checkout span span { background:none; display:block; line-height:0; font-size:0; text-indent:-9999em; }
button.btn-checkout.no-checkout {}

#review-buttons-container button.btn-checkout span { background:transparent url(../images/submit-order.png) no-repeat 0 0; width:151px; height:39px; padding:0; }

p.control input.checkbox,
p.control input.radio { margin-right:6px; }
