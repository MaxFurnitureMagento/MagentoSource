/********** < Navigation */

.nav-container {position:relative;}
#nav {height: 43px; padding:0 0; margin:10px 0; font-size:13px; background: url('../images/ui/navigation-background.png') repeat-x bottom left; color: #fff}
/* All Levels */
#nav li { text-align:left;  float: left }
#nav li.over { z-index:998; }
#nav li.parent {}
#nav li a { display:block; text-decoration:none; }
#nav li a:hover { text-decoration:none; }
#nav li a span { display:block; white-space:nowrap; cursor:pointer; }
#nav li ul a span { white-space:normal; }

/* 1st Level */
#nav li { float:left; }
#nav li a { float:left; height:33px; padding:0 0 10px 10px; font:Arial; font-size: 12px; font-weight:bold; color:#fff; }
#nav li a:hover { color:#fff; }
#nav li.over a,
#nav li.active a { color:#fff; }

/* 2nd Level */
#nav ul,
#nav div { position:absolute; top:43px; min-height:318px; left:-10000px; background: url(../images/mid_children_navigation.jpg) repeat-y white; }
#nav div ul { position:static; margin: 0; width:165px; min-height:250px; background:transparent url(../images/background/dropdown-shadow.jpg) no-repeat 100% 0; padding:0 60px 0 30px; }
#nav div .category-title { font:normal 18px Georgia, Times, serif; color:#0b3889; margin-left:29px; display:block; }
#nav div ul li a { padding:0; height:26px; line-height:26px; border-bottom:1px dotted #c6c2c3; }
#nav div ul li.last a { padding:0; }
#nav div ul li a span { padding:0; }
#nav div ul .view-all a { border:none; margin-top:5px; font-weight:bold; color:#7e7577; }
#nav div .featured-products { position:absolute; left:330px; top:23px; background:none; min-height:270px; height:270px; }
#nav div .featured-products .category-title { margin-left:0; margin-bottom: 20px; }
#nav div .product { position:static; float:left; background:none; border:1px solid #c9c9c9; padding:5px; height:190px; min-height:190px; width:190px; margin-right:10px; }
#nav div .product .product-name { color:#0f49ad; font-size:11px; font-weight:normal; line-height:14px; float:none; display:block; padding:5px 0 0; white-space:normal; }
#nav div .product a { background:none; padding:0; height:auto; display:block; }
#nav div .product a:hover { background:none; }
#nav div a.view-more { position:absolute; bottom:0; right:0; color:#0f49ad; font-weight:bold; height:auto; padding:0; margin-right:10px; text-decoration:underline; }
#nav div a.view-more:hover { background:none; text-decoration:none; }

/* 3rd+ level */
#nav ul ul,
#nav ul div { top:7px; }

#nav div.shown-sub .top{background: url(../images/header.png) no-repeat 0  -183px; width:1000px; height: 24px; display:block; }

#nav div.shown-sub .bottom{background: url(../images/header.png) no-repeat 0  -210px; width:1000px; height: 32px; display:block; }
#nav ul li.last ,
#nav ul li a:hover {background: none}

#nav ul li { float:none; }
#nav ul li.last { border-bottom:0; }
#nav ul li a { float:none; padding:3px 9px; font-weight:normal; color:#888 !important; }
#nav ul li a:hover { color:#000 !important; }
#nav ul li.active > a,
#nav ul li.over > a { color:#000 !important; }

/* Show menu */
#nav li ul.shown-sub,
#nav li div.shown-sub {  z-index:999;left:-5px }
#nav li .shown-sub ul.shown-sub,
#nav li .shown-sub li div.shown-sub { left:100px;  }

#nav li.last {}
