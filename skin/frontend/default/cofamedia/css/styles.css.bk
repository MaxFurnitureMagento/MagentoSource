#nav li .arrow{background: url(../images/header.png) -349px -8px; height: 6px; width: 9px;display: block;padding-top: 0; margin: 17px 4px 0 0;}
#nav li .separator{background: url(../images/header.png) -382px 0px; height: 43px; width: 2px;display: block;padding-top: 0;  margin: 0}
